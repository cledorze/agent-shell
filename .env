# General configuration
LOG_LEVEL=info

# API Gateway
API_PORT=8080

# OpenAI
OPENAI_API_KEY=

# ngrok
NGROK_AUTH_TOKEN=
NGROK_REGION=eu

# AnythingLLM
ANYTHINGLLM_URL=http://anythingllm:3001
ANYTHINGLLM_API_KEY=your_anythingllm_api_key_here

# Orchestrator
ORCHESTRATOR_PORT=8081

# VM Manager
VM_MANAGER_PORT=8083
LIBVIRT_URI=qemu:///system
VM_TEMPLATE_PATH=/var/lib/libvirt/images/opensuse-tumbleweed-template.qcow2
VM_DATA_DIR=/var/lib/linux-agent-system/vms
ENABLE_SIMULATION=false

# Command Executor
COMMAND_EXECUTOR_PORT=8085
